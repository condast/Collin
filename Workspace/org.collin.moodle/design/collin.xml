<?xml version="1.0" encoding="UTF-8"?>
<collin id="org.collin.dashboard.test" name="Collin Test"
	title="HowAComputer.Thinks">
	<units>
		<unit id="org.collin.dashboard.test">
			<!-- This tetra describes the approach of the provider -->
			<tetra type="moodle-course">
				<nodes>
					<function type="supplier" activity="1.1"></function>
					<goal type="module">Introduction in the area</goal>
					<task type="paragraph" time="1 hr" progress="80"/>
				</nodes>				
				<ambition id="module.introduction" description="Describes the ambition"/>
				<operation name="development" description="Describes the approach">
						<function type="Course Programming"></function>
						<task></task>
						<solution></solution>
				</operation>
				<learning description="Describes the goals of learnng">
						<function></function>
						<goal></goal>
						<task></task>
				</learning>
				<recovery type="knowledge" description="Describes the reflection afterwards">
						<goal></goal>
						<task></task>
						<solution></solution>
				</recovery>
  		</tetra>
  		<!-- This tetra monitors the student -->
 			<tetra type="moodle" name="student">
				<nodes>
					<function type="subject"></function>
					<goal type="question">What is your goal</goal>
					<task type="question">How will you achieve this</task>
					<solution></solution>
				</nodes>
				<ambition id="module.introduction" question="why, where?"/>
				<learning question="how?"/>
				<operation name="development" question="what, when?"/>
				<recovery type="knowledge" question="reflect?"/>
 		</tetra>
  		<!-- This tetra provides the coach of the supplier, with a focus on achieving the goals -->
 			<tetra type="team zodiac" name="tutor">
				<nodes>
					<function type="coach-supplier"></function>
					<goal></goal>
					<task></task>
					<solution></solution>
				</nodes>
				<ambition id="coach.ponte"/>
				<learning/>
				<operation/>
				<recovery/>
  		</tetra>
   		<!-- This tetra provides the coach of the consumer, with a focus on keeping the student happy -->
 			<tetra type="team Zodiac" name="support">
				<nodes>
					<function type="coach-subject"></function>
					<goal></goal>
					<task></task>
					<solution></solution>
				</nodes>
				<ambition id="coach.ponte"/>
				<learning/>
				<operation/>
				<recovery/>
  		</tetra>
  		<!-- This tetra provides the product of the supplier -->
 			<tetra type="product" name="exam">
				<nodes>
					<function type="product"></function>
					<goal></goal>
					<task></task>
					<solution></solution>
				</nodes>
				<ambition id="certification"/>
				<learning/>
				<operation/>
				<recovery/>
  		</tetra>
  		<!-- This tetra provides the process of the student -->
 			<tetra type="project plan">
				<nodes>
					<function type="process"></function>
					<goal></goal>
					<task></task>
					<solution></solution>
				</nodes>
				<ambition id="pva"/>
				<learning description="planning"/>
				<operation/>
				<recovery/>
  		</tetra>
   	</unit>
  </units>
</collin>